

SELECT COLETA, RECEBIDO
FROM AMOSTRAS

SELECT COUNT(COLETA), COLETA
FROM AMOSTRAS
WHERE COLETA LIKE '%/13%' OR COLETA LIKE '%/2013%'
GROUP BY ROLLUP(COLETA)
--119 - 4 - 8 - 2 - 3 = 102


SELECT COUNT(COLETA), COLETA
FROM AMOSTRAS
WHERE COLETA LIKE '%/14%' OR COLETA LIKE '%/2014%'
GROUP BY ROLLUP(COLETA)
--12


SELECT COUNT(COLETA), COLETA
FROM AMOSTRAS
WHERE COLETA LIKE '%/17%' OR COLETA LIKE '%/2017%'
GROUP BY ROLLUP(COLETA)
--326


SELECT COUNT(COLETA), COLETA
FROM AMOSTRAS
WHERE COLETA LIKE '%/18%' OR COLETA LIKE '%/2018%'
GROUP BY ROLLUP(COLETA)
--98


SELECT COUNT(COLETA), COLETA
FROM AMOSTRAS
WHERE COLETA LIKE ''
GROUP BY ROLLUP(COLETA)