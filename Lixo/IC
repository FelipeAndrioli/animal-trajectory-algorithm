CREATE TABLE AMOSTRAS(
	ID VARCHAR(10),
	BANCO VARCHAR(15),
	NEC VARCHAR(6),
	SEXO CHAR(1),
	EC VARCHAR(5),
	DNA FLOAT(5),
	COLETA DATE,
	GENERO VARCHAR(20),
	ESPECIE VARCHAR(50),
	NOMECOMUM VARCHAR(50),
	GEO GEOMETRY(POINT, 4326),
	RECEBIDO VARCHAR(50),
	LOCAL VARCHAR(50),
	MUNICIPIO VARCHAR(30),
	ST VARCHAR(2),
	PAIS VARCHAR(10),
	SEMREGISTROI VARCHAR(10),
	SEMREGISTROII VARCHAR(10),
	COLETORCAM VARCHAR(20),
	COLETORLAB VARCHAR(20)
	
);

